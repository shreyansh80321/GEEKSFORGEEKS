class Solution {
  public:
    int missingNum(vector<int>& arr) {
        // code here
        int i=0;
        sort(arr.begin(),arr.end());
        for(i=0;i<arr.size();i++)
        {
            if(arr[i]!=(i+1))
            {
                return i+1;
            }
        }
        return i+1;
    }
};
